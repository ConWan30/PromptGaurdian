{
  "name": "promptguardian",
  "version": "1.0.0",
  "description": "Real-time AI prompt threat detection with SocialShield monitoring",
  "main": "railway-backend/src/server.js",
  "scripts": {
    "build": "echo 'No build step required for backend deployment'",
    "dev": "webpack --mode=development --watch",
    "test": "jest",
    "lint": "eslint src/",
    "start": "cd railway-backend && npm start",
    "extension:build": "webpack --mode=production",
    "railway:dev": "cd railway-backend && npm run dev",
    "railway:build": "cd railway-backend && npm run build",
    "railway:start": "cd railway-backend && npm start",
    "postinstall": "cd railway-backend && npm install"
  },
  "keywords": [
    "browser-extension",
    "ai-security",
    "prompt-injection",
    "threat-detection",
    "social-media-security"
  ],
  "author": "ConWan30",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0"
  }
}